{
  "name": "loot",
  "version": "5.0.4",
  "description": "Wrapper around the LOOT library for sorting plugins of gamebryo games (Oblivion, Skyrim, Fallout 3/NV/4)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nexus-Mods/node-loot.git"
  },
  "scripts": {
    "upload": "prebuild.cmd -r napi -t 4 -a x64 --prepack codesign",
    "rebuild": "node libloot.js && node-gyp rebuild",
    "install": "prebuild-install -r napi -t 4 -a x64 || npm run rebuild"
  },
  "author": "Black Tree Gaming Ltd.",
  "license": "GPL-3.0",
  "binary": {
    "napi_versions": [
      4
    ]
  },
  "devDependencies": {
    "prebuild": "^11.0.3"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "lodash": "^4.17.21",
    "node-7z": "Nexus-Mods/node-7z",
    "node-addon-api": "^5.0.0",
    "node-gyp": "^9.0.0",
    "prebuild-install": "^7.1.0",
    "tar": "^6.1.13"
  },
  "resolutions": {
    "node-gyp": "^9.0.0"
  }
}
